{
    "name": "chabiselx/debuggertools",
    "description": "Create a log for understand algorithm",
    "type": "library",
    "require": {
        "php": "^7 || ^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "doctrine/orm": "^2.19",
        "symfony/cache": "^5.4",
        "doctrine/annotations": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Debuggertools\\": "src/Debuggertools/"
        },
        "exclude-from-classmap": [
            "/docker",
            ".env",
            ".env.local",
            "docker-compose.yml",
            "/tests",
            "phpunit.xml",
            "/makeFileFolder",
            "MakeFile"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "BISELX Charles",
            "email": "cha.biselx@gmail.com"
        }
    ],
    "archive": {
        "exclude": [
            "/docker",
            ".env",
            ".env.local",
            "docker-compose.yml",
            "/tests",
            "phpunit.xml",
            "/makeFileFolder",
            "MakeFile"
        ]
    }
}
